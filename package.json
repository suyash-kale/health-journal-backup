  },
  "devDependencies": {
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.2",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.2.0",
    "husky": "^7.0.4",
    "lerna": "^4.0.0",
    "semantic-release": "^18.0.1"
  }
}
